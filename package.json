{
  "name": "genesys-dashboard",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://doctorhcode333.github.io/genesys-dashboard",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:v1.0.0": "cross-env VERSION=1.0.0 npm run build",
    "build:v2.0.0": "cross-env VERSION=2.0.0 npm run build",
    "build:latest": "cross-env VERSION=latest npm run build",
    "predeploy": "npm run build:latest",
    "deploy": "npm run copy:latest && gh-pages -d dist",
    "copy:latest": "xcopy /E /I /Y dist\\vlatest\\* dist\\", 
    "deploy:versions": "npm run build && git add dist && git commit -m 'Deploy new versions' && git push origin versions",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.15.21",
    "@mui/material": "^5.16.4",
    "@mui/x-charts": "^7.8.0",
    "@mui/x-data-grid": "^7.8.0",
    "@mui/x-date-pickers": "^7.8.0",
    "@mui/x-tree-view": "^7.8.0",
    "@reduxjs/toolkit": "^2.2.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "purecloud-platform-client-v2": "^196.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.25.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.1"
  }
}
